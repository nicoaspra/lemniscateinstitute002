\[\begin{align*}
	\frac{d}{dt}(w) &amp;= \frac{d}{dt}\br{ \frac{1}{t^2}-\frac{1}{t^5} } \\
	\frac{dw}{dt} &amp;= \frac{d}{dt}(t^{-2})-\frac{d}{dt}(t^{-5})	\qquad \tcA{\frac{1}{x^n}=x^{-n}} \\
	&amp;= (-2)t^{-2-1}-(-5)t^{-5-1} \\
	&amp;= -2t^{-3}+5t^{-6} \\
	&amp;= -\frac{2}{t^3}+\frac{5}{t^6}		\tagans
\end{align*}\]

